{
    "manifest_version": 3,
    "name": "unloosen-dev",
    "description": "this extension is built by unloosen with ruby!",
    "version": "0.0.1",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "packages/unloosen-ruby/dist/entry/init-bg.esm.js",
        // "service_worker": "background.html",
        "type": "module"
    },
    "content_scripts": [
        {
            "js": [
                "packages/unloosen-ruby/dist/entry/loader-cs.esm.js"
            ],
            "matches": ["http://www.example.com/"]
        }
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    },
    "web_accessible_resources": [
        {
          "resources": ["*"],
          "matches": ["<all_urls>"]
        }
    ]
}